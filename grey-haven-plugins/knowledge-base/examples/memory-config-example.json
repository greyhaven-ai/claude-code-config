{
  "embedding_provider": "openai",
  "embedding_model": "text-embedding-3-small",
  "embedding_dimensions": 1536,
  "search_default_k": 5,
  "auto_embed": true,
  "dataset_path": ".claude/memory/contextframe.lance",

  "providers": {
    "openai": {
      "models": {
        "text-embedding-3-small": {
          "dimensions": 1536,
          "max_tokens": 8191
        },
        "text-embedding-3-large": {
          "dimensions": 3072,
          "max_tokens": 8191
        }
      },
      "api_key_env": "OPENAI_API_KEY"
    },
    "anthropic": {
      "models": {
        "claude-3-sonnet": {
          "note": "Anthropic doesn't provide dedicated embedding models, use voyage-ai or OpenAI"
        }
      },
      "api_key_env": "ANTHROPIC_API_KEY"
    }
  },

  "search": {
    "default_k": 5,
    "max_k": 20,
    "relevance_threshold": 0.7,
    "bm25_k1": 1.2,
    "bm25_b": 0.75
  },

  "memory": {
    "default_status": "active",
    "auto_archive_after_days": 365,
    "commit_types": [
      "implementation",
      "decision",
      "incident",
      "learning",
      "milestone"
    ]
  },

  "performance": {
    "cache_embeddings": true,
    "cache_directory": ".claude/memory/embeddings/",
    "batch_size": 10,
    "parallel_processing": true
  },

  "git_integration": {
    "auto_capture_commits": true,
    "auto_capture_branch": true,
    "auto_capture_files": true
  }
}
